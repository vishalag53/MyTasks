<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/clImport3"
        android:layout_width="match_parent"
        android:layout_height="50dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clImport4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@id/clImport3"
            app:layout_constraintEnd_toEndOf="@id/clImport3"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@id/clImport3"
            app:layout_constraintTop_toTopOf="@id/clImport4">

            <ImageView
                android:id="@+id/imgViewIcon3"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="8dp"
                style="@style/listImage"
                app:layout_constraintBottom_toBottomOf="@id/clImport4"
                app:layout_constraintStart_toStartOf="@id/clImport4"
                app:layout_constraintTop_toTopOf="@id/clImport4"
                android:importantForAccessibility="no" />

            <TextView
                android:id="@+id/textViewTitle3"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                tools:text="List Name"
                android:textSize="20sp"
                android:layout_marginStart="8dp"
                style="@style/textColor"
                android:paddingStart="4dp"
                android:paddingEnd="4dp"
                android:paddingBottom="2dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imgViewIcon3"
                app:layout_constraintTop_toTopOf="parent"/>

            <TextView
                android:id="@+id/textViewTotalNumber3"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_marginTop="2dp"
                android:layout_marginEnd="8dp"
                android:gravity="center"
                android:text="@string/_8"
                android:textAlignment="center"
                style="@style/textColor"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@id/clImport4"
                app:layout_constraintEnd_toEndOf="@id/clImport4"
                app:layout_constraintTop_toTopOf="@id/clImport4" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>